{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "headedTest": "npx cypress run --headed",
    "recordDashBordTest": "npx cypress run --record --key cd6017a9-8732-4b30-932a-caa0a6234cf9",
    "specificFolderRun": "npx cypress run --spec \"cypress/integration/examples/Basics/*\"",
    "FormValidationTest": "npx cypress run --spec \"cypress/integration/examples/BDD/Form.feature\"",
    "FormValidationTestWithAllure": "npx cypress run --spec \"cypress/integration/examples/BDD/Form.feature\" && allure generate allure-results --output allure-report",
    "FormValidationTestWithDashboard": "npx cypress run --spec \"cypress/integration/examples/BDD/Form.feature\" --headed --browser chrome",
    "Allure": "npx allure generate allure-results --clean -o allure-report && npx allure open allure-report"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.2",
    "allure-commandline": "^2.30.0",
    "cypress": "^13.14.2",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "mocha-allure-reporter": "^1.4.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@cypress/browserify-preprocessor": "latest"
  }
}
